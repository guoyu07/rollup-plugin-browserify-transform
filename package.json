{
  "name": "rollup-plugin-browserify-transform",
  "version": "0.1.0",
  "description": "Use Browserify transforms with Rollup",
  "keywords": [
    "rollup-plugin",
    "browserify"
  ],
  "license": "MIT",
  "homepage": "https://github.com/lautis/rollup-plugin-browserify-transform",
  "author": "Ville Lautanala",
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-register": "^6.3.13",
    "brfs": "^1.0.0",
    "coffeeify": "^3.0.0",
    "coffeescript": "^2.0.1",
    "mocha": "^4.0.1",
    "rollup": "^0.37.0",
    "rollup-plugin-babel": "^3.0.2"
  },
  "scripts": {
    "build": "rm -rf dist/* && BABEL_ENV=production rollup -c -f cjs -o dist/rollup-plugin-browserify-transform.cjs.js && BABEL_ENV=production rollup -c -f es6 -o dist/rollup-browserify-transform.es6.js",
    "test": "mocha --require babel-register",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "concat-stream": "^1.5.1",
    "object-assign": "^4.0.1",
    "rollup-pluginutils": "^2.0.1",
    "source-map-url": "^0.4.0"
  },
  "files": [
    "README.md",
    "LICENSE.txt",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lautis/rollup-plugin-browserify-transform.git"
  },
  "bugs": {
    "url": "https://github.com/lautis/rollup-plugin-browserify-transform/issues"
  },
  "main": "dist/rollup-plugin-browserify-transform.cjs.js",
  "jsnext:main": "dist/rollup-plugin-browserify-transform.es6.js"
}
